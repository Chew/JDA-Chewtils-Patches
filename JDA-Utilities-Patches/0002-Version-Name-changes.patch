From 55988f13bd4f9ab6caf13cdb06ade72b05f49fa4 Mon Sep 17 00:00:00 2001
From: Chew <chew@chew.pw>
Date: Wed, 30 Sep 2020 15:02:57 -0500
Subject: [PATCH] Version/Name changes


diff --git a/build.gradle b/build.gradle
index 571121f..c6caadf 100644
--- a/build.gradle
+++ b/build.gradle
@@ -22,7 +22,7 @@ plugins {
     id 'com.jfrog.bintray' version '1.8.4'
 }
 
-def versionInfo = [major: '3', minor: '0', revision: '4']
+def versionInfo = [major: '1', minor: '0', revision: '0']
 
 task build {
     group = 'build'
@@ -33,7 +33,7 @@ allprojects {
     apply plugin: 'com.jfrog.bintray'
     apply plugin: 'com.github.johnrengelman.shadow'
 
-    group = 'com.jagrosh'
+    group = 'pw.chew'
     version = versionInfo.values().join('.')
 
     ext {
diff --git a/settings.gradle b/settings.gradle
index 869ba6a..5783c90 100644
--- a/settings.gradle
+++ b/settings.gradle
@@ -13,7 +13,7 @@
  * See the License for the specific language governing permissions and
  * limitations under the License.
  */
-rootProject.name = 'JDA-Utilities'
+rootProject.name = 'JDA-Chewtils'
 
 include ':command'
 include ':commons'
